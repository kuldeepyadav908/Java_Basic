package saket;
import java.io.IOException;
import java.lang.*;
public class Star1 {
	public static void main(String arg[])throws IOException,ArrayIndexOutOfBoundsException
	{
		int n = Integer.parseInt(arg[0]);
				int i,j;
		for(i = 0;i < n; i++)
		{
			for(j=0;j <= i;j++)
			{
				System.out.print('*');
				//System.out.println(" ");
			}
			System.out.println("\n");
				
		}
	}

}
